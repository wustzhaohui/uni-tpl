  "uni-app": {// 扩展配置
  "scripts": {
      "custom-platform": { //自定义编译平台配置，可通过cli方式调用
          "title":"自定义扩展名称", // 在HBuilderX中会显示在 运行/发行 菜单中
          "BROWSER":"",  //运行到的目标浏览器，仅当UNI_PLATFORM为h5时有效
          "env": {//环境变量
              "UNI_PLATFORM": "",  //基准平台
              "MY_TEST": "", // ... 其他自定义环境变量
              "ANY_PARMS": "",
              "XXX": "YYY"
           }
      }
  }    
}
}
